artItemData = this.cart_item_data;\n\tcartItemData.quantity = 1;// добавил количество \n    }\n}\n</script>\n<style lang=\"scss\">\n    @import './src/assets/styles/styles.scss';// нужно импортироватьтак потому что по другому не работает будет ошибка\n.v-cart-item{\n    display: flex;\n    flex-wrap: nowrap;\n    justify-content: space-between ;\n    align-items: center;\n    box-shadow: 0 0 8px 0 #979393;\n    padding:$padding*2;\n    margin: $margin*2;\n    .v-cart-item-img{\n\n        max-width: 50px ;\n    \n    }\n}\n</style>"],"mappings":"AA6BA,SAASA,UAAS,QAAS,MAAK;AAChC,eAAe;EACXC,IAAI,EAAC,aAAa;EAClBC,KAAK,EAAC;IACFC,cAAc,EAAC;MACXC,IAAI,EAAEC,MAAM;MACZC,OAAOA,CAAA,EAAE;QACL,OAAM,CAAC;MACX;IACJ;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,GAAGP,UAAU,CACT,CAAC,oBAAoB,EAAC,oBAAoB,EAC7C;IACD;IACA;IACAQ,QAAQA,CAAA,EAAE;MACN,IAAI,CAACC,KAAK,CAAC,UAAU;IACzB;EACJ,CAAC;EACDC,QAAQA,CAAA,EAAE,CAAC,CAAC;EACZC,OAAOA,CAAA,EAAE;IACL,MAAMC,YAAW,GAAI,IAAI,CAACT,cAAc;IAC/CS,YAAY,CAACC,QAAO,GAAI,CAAC,CAAC;EACvB;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}